<?import javafx.scene.text.Font?>


<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="schedulingapp_heathersmith.View.ConsultantSchedReportController">
   <center>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="consultantSchedTable" prefHeight="407.0" prefWidth="692.0" HBox.hgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="startCol" prefWidth="147.33335876464844" text="Start" />
                <TableColumn fx:id="endCol" prefWidth="135.33331298828125" text="End" />
                  <TableColumn fx:id="titleCol" prefWidth="98.66668701171875" text="Title" />
                  <TableColumn fx:id="custNameCol" prefWidth="187.33328247070312" text="Customer Name" />
                  <TableColumn fx:id="contactCol" prefWidth="147.33335876464844" text="Contact" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </HBox>
   </center>
   <top>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="200.0" BorderPane.alignment="CENTER_LEFT">
         <children>
            <Button fx:id="homeBtn" mnemonicParsing="false" text="Home" />
            <Label fx:id="consultantSchedLabel" alignment="CENTER" text="Consultant Schedule" HBox.hgrow="ALWAYS">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </HBox>
   </top>
</BorderPane>
